package Extend.test;

public class TestExtends {
	/*
	 * 只有单继承
	 * 多继承要通过接口实现
	 * extends
	 * 默认继承Object
	 */
	
	class Animal{
		String name;
		public void  sleep(){
			System.out.print(name);
		}
		public void run(){
			System.out.print("跑");
		}
	}
	
	class Bird extends Animal {
		
		public void run() { // 重写方法
			super.run(); // 通过super 调用父类的run
			System.out.print("飞");
		}
	}
}
