
public class Str {
	public static void main(String[] args) {
		// char 可以和数字之间进行转换
		// char a = (char)10;
		
		char a = (char)(70);
		
		System.out.print(a);
		
		/*
		 * 自动类型转换
		 * byte -> short -> int -> long
		 * char -> int
		 * int -> double
		 * float -> double
		 * 
		 * int -> float   long -> float  long -> double   有可能造成精度丢失
		 * */
		
		char c = 50;
	}
}
